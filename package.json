{
  "name": "eslint-plugin-perfectionist",
  "description": "ESLint plugin for sorting various data such as objects, imports, types, enums, JSX props, etc.",
  "type": "commonjs",
  "version": "3.9.1",
  "homepage": "https://perfectionist.dev",
  "repository": "azat-io/eslint-plugin-perfectionist",
  "author": "Azat S. <to@azat.io>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "perfectionist"
  ],
  "scripts": {
    "build": "vite build",
    "docs:dev": "astro dev",
    "docs:build": "astro build",
    "release": "pnpm release:check && pnpm release:version && pnpm release:git",
    "release:check": "pnpm test && pnpm run build",
    "release:git": "pnpm release:git:add && pnpm release:git:commit && pnpm release:git:tag && pnpm release:git:push",
    "release:git:add": "git add .",
    "release:git:commit": "git commit -m \"build: publish v$(node -p \"require('./package.json').version\")\"",
    "release:git:tag": "git tag v$(node -p \"require('./package.json').version\")",
    "release:git:push": "git push --follow-tags && git push origin v$(node -p \"require('./package.json').version\")",
    "release:version": "changelogen --output changelog.md --release --no-commit --no-tag",
    "ci:changelog": "changelogithub",
    "ci:clear": "clear-package-json package.json --output package.json",
    "test:astro": "astro check",
    "test:format": "prettier --check \"**/*.{astro,svelte,js,ts,json,md,yml}\"",
    "test:css": "stylelint \"docs/**/*.{astro,svelte,css}\"",
    "test:js": "eslint \"**/*.{astro,svelte,js,ts}\"",
    "test:spelling": "cspell \"**/*\"",
    "test:svelte": "svelte-check",
    "test:types": "tsc --noEmit --pretty",
    "test:unit": "vitest --run --coverage",
    "test": "pnpm run /^test:/"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/types": "^8.13.0",
    "@typescript-eslint/utils": "^8.13.0",
    "natural-compare-lite": "^1.4.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^3.1.9",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/svelte": "^5.7.3",
    "@azat-io/eslint-config-astro": "^1.10.0",
    "@azat-io/stylelint-config": "^0.1.1",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@nanostores/persistent": "^0.10.2",
    "@nanostores/vue": "^0.10.0",
    "@playform/compress": "^0.1.6",
    "@poppanator/sveltekit-svg": "5.0.0",
    "@shikijs/transformers": "^1.22.2",
    "@types/mdast": "^4.0.4",
    "@types/natural-compare-lite": "^1.4.2",
    "@types/node": "^22.9.0",
    "@types/unist": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@typescript-eslint/rule-tester": "^8.13.0",
    "@typescript-eslint/types": "^6.13.0",
    "@vercel/og": "^0.6.3",
    "@vitest/coverage-v8": "^2.1.4",
    "astro": "^4.16.10",
    "astro-eslint-parser": "^1.0.3",
    "browserslist": "^4.24.2",
    "changelogen": "^0.5.7",
    "changelogithub": "^0.13.11",
    "clean-publish": "^5.1.0",
    "cspell": "^8.16.0",
    "eslint": "^9.14.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-eslint-plugin": "^6.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.13.1",
    "eslint-plugin-node-import": "^1.0.4",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prefer-let": "^4.0.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-sonarjs": "2.0.4",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "execa": "^9.5.1",
    "keyux": "^0.10.0",
    "lightningcss": "^1.28.1",
    "nanostores": "^0.11.3",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.2.7",
    "rehype-external-links": "^3.0.0",
    "remark-sectionize": "^2.0.0",
    "shiki": "^1.22.2",
    "shiki-magic-move": "^0.5.0",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.10.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-gamut": "^1.3.4",
    "stylelint-order": "^6.0.4",
    "stylelint-plugin-logical-css": "^1.2.1",
    "svelte": "5.1.13",
    "svelte-check": "^4.0.6",
    "svelte-eslint-parser": "^0.43.0",
    "svgo": "^3.3.2",
    "ts-dedent": "^2.2.0",
    "typescript": "^5.6.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vite": "^5.4.10",
    "vite-plugin-dts": "4.3.0",
    "vite-plugin-lightningcss": "^0.0.5",
    "vitest": "^2.1.4"
  }
}
