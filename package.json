{
  "name": "eslint-plugin-perfectionist",
  "description": "ESLint plugin for sorting various data such as objects, imports, types, enums, JSX props, etc.",
  "version": "3.9.1",
  "homepage": "https://perfectionist.dev",
  "repository": "azat-io/eslint-plugin-perfectionist",
  "author": "Azat S. <to@azat.io>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "perfectionist"
  ],
  "scripts": {
    "build": "vite build",
    "docs:dev": "astro dev",
    "docs:build": "astro build",
    "release": "pnpm release:check && pnpm release:version && pnpm release:publish",
    "release:check": "pnpm test && pnpm run build",
    "release:publish": "clean-publish",
    "release:version": "changelogen --output changelog.md --release --push",
    "test:astro": "astro check",
    "test:format": "prettier --check \"**/*.{astro,svelte,js,ts,json,md,yml}\"",
    "test:css": "stylelint \"docs/**/*.{astro,svelte,css}\"",
    "test:js": "eslint \"**/*.{astro,svelte,js,ts}\"",
    "test:spelling": "cspell \"**/*\"",
    "test:svelte": "svelte-check",
    "test:types": "tsc --noEmit --pretty",
    "test:unit": "vitest --run --coverage",
    "test": "pnpm run /^test:/"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "peerDependenciesMeta": {
    "astro-eslint-parser": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "peerDependencies": {
    "astro-eslint-parser": "^1.0.2",
    "eslint": ">=8.0.0",
    "svelte": ">=3.0.0",
    "svelte-eslint-parser": "^0.41.1",
    "vue-eslint-parser": ">=9.0.0"
  },
  "dependencies": {
    "@typescript-eslint/types": "^8.9.0",
    "@typescript-eslint/utils": "^8.9.0",
    "minimatch": "^9.0.5",
    "natural-compare-lite": "^1.4.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^3.1.8",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/svelte": "^5.7.2",
    "@azat-io/eslint-config-astro": "^1.10.0",
    "@azat-io/stylelint-config": "^0.1.1",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@nanostores/persistent": "^0.10.2",
    "@nanostores/vue": "^0.10.0",
    "@playform/compress": "^0.1.4",
    "@poppanator/sveltekit-svg": "5.0.0-svelte5.5",
    "@shikijs/transformers": "^1.22.0",
    "@types/mdast": "^4.0.4",
    "@types/natural-compare-lite": "^1.4.2",
    "@types/node": "^22.7.5",
    "@types/unist": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "@typescript-eslint/rule-tester": "^8.9.0",
    "@typescript-eslint/types": "^6.13.0",
    "@vercel/og": "^0.6.3",
    "@vitest/coverage-v8": "^2.1.3",
    "astro": "^4.16.4",
    "astro-eslint-parser": "^1.0.3",
    "browserslist": "^4.24.0",
    "changelogen": "^0.5.7",
    "clean-publish": "^5.0.0",
    "cspell": "^8.15.2",
    "eslint": "^9.12.0",
    "eslint-plugin-astro": "^1.3.0",
    "eslint-plugin-eslint-plugin": "^6.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-n": "^17.11.1",
    "eslint-plugin-node-import": "^1.0.4",
    "eslint-plugin-perfectionist": "^3.8.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prefer-let": "^4.0.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-sonarjs": "2.0.3",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "execa": "^9.4.0",
    "keyux": "^0.9.0",
    "lightningcss": "^1.27.0",
    "nanostores": "^0.11.3",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.2.7",
    "rehype-external-links": "^3.0.0",
    "remark-sectionize": "^2.0.0",
    "shiki": "^1.22.0",
    "shiki-magic-move": "^0.5.0",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.10.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-gamut": "^1.3.4",
    "stylelint-order": "^6.0.4",
    "stylelint-plugin-logical-css": "^1.2.1",
    "svelte": "5.0.0-next.221",
    "svelte-check": "^4.0.5",
    "svelte-eslint-parser": "^0.41.1",
    "svgo": "^3.3.2",
    "ts-dedent": "^2.2.0",
    "typescript": "^5.6.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vite": "^5.4.9",
    "vite-plugin-dts": "4.2.4",
    "vite-plugin-lightningcss": "^0.0.5",
    "vitest": "^2.1.3",
    "vue-eslint-parser": "^9.4.3"
  }
}
